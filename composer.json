{
    "name": "krak/http",
    "description": "Http Utilities for building frameworks",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/krakphp/http",
    "keywords": [
        "utility",
        "psr-7",
        "http",
        "routing"
    ],
    "authors": [
        {
            "name": "RJ Garcia",
            "email": "rj@bighead.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Krak\\Http\\": "src"
        },
        "files": ["src/inc.php"]
    },
    "require": {
        "nikic/iter": "^1.4",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.3",
        "krak/cargo": "^0.2.4",
        "krak/mw": "^0.5.0",
        "krak/php-inc": "^0.1.3",
        "nikic/fast-route": "^1.0",
        "peridot-php/peridot": "^1.18",
        "symfony/var-dumper": "^3.2",
        "zendframework/zend-diactoros": "^1.3"
    },
    "suggest": {
        "nikic/fast-route": "For the FastRoute dispatcher"
    }
}
